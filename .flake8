[flake8]
max-line-length = 100
extend-ignore = 
    E203,
    W503,
    E501,
    F401
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    *.egg-info,
    .pytest_cache,
    .mypy_cache,
    migrations
per-file-ignores =
    __init__.py:F401
    tests/*:S101,S106
max-complexity = 10

